        </nav>

    </header>
    <div class="content-container" id="content-container">
        <div class="content-box-1 content">
            <div class="content-title">
                <h2>Verjaardag</h2>
            </div>
            <% apiPerson.data.forEach(data=> { %>

                <div class="person-container">
                    <div class="person-avatar">
                        <% if (data.avatar) { %>
                            <svg version="1.1" x="0px" y="0px" viewBox="0 0 100 100" xml:space="preserve">
                                <path class="st0"
                                    d="M70.2,39c0,11.2-9.1,20.4-20.4,20.4S29.5,50.2,29.5,39c0-11.2,9.1-20.4,20.4-20.4S70.2,27.8,70.2,39z" />

                                <path class="st0" d="M55.2,80.7C55.2,80.6,55.3,80.6,55.2,80.7L55.2,80.7z" />
                                <path class="st0" d="M50,0C22.4,0,0,22.4,0,50c0,12.6,4.7,24.7,13.3,33.9c0,0,0.1,0.1,0.1,0.2l0,0c4.7,5,10.2,9,16.5,11.7
			c6.3,2.8,13.1,4.2,20,4.2h0.1c6.9,0,13.5-1.4,19.8-4.1c6.3-2.7,11.9-6.7,16.6-11.7c0,0,0.1-0.1,0.1-0.1c8.7-9.3,13.4-21.4,13.4-34
			C100,22.4,77.6,0,50,0z M4,50C4,24.6,24.6,4,50,4s46,20.6,46,46c0,10.5-3.5,20.6-10.1,28.7c-2.5-6.9-8.4-14.4-22.1-17.4
			c-0.3-0.1-0.7-0.1-1-0.1c-0.8,0-1.6,0.2-2.3,0.6c-3.2,1.7-6.9,2.6-10.5,2.6h-0.2c-5.4,0-9.2-1.8-10.6-2.6c-1-0.6-2.2-0.8-3.4-0.5
			h0c-13.5,2.9-19.4,10.4-22,17.2C7.5,70.4,4,60.4,4,50z M55.2,80.7C55.2,80.6,55.3,80.6,55.2,80.7L55.2,80.7z" />
                            </svg>
                            <% } else { %>
                                <%= data.avatar.initials %>

                                    <% } %>
                    </div>

                    <div class="person-birth-day">
                        <% if (data.full_name) { %>
                            <p>
                                <%= data.full_name %>
                                    <% } else { %>
                                        no name
                                        <% } %>
                                            <b>
                                                <%= (new Date().getFullYear() - data.date_of_birth.slice(0, 4)) %> jaar
                                            </b>

                            </p>
                            <span class="age">

                                is jarig op:

                                <% if (data.date_of_birth) { %>
                                    <%= data.date_of_birth.slice(8, 10) %>-<%= data.date_of_birth.slice(6, 7) %>-<span
                                                class="current-year">
                                                <%= data.date_of_birth.slice(0, 4) %>
                                            </span>
                                            <% } else { %>
                                                prive
                                                <% } %>
                            </span>
                    </div>
                </div>
                <% }) %>
        </div>

        <div class="content-box-2 content">
            <div class="content-title">
                <h2>absent</h2>
            </div>
            <% if (apiAbsence !=undefined && apiAbsence.data.length> 0) { %>
                <% apiAbsence.data.forEach(data=> { %>
                    <div class="abcente-list">
                        <% if (data.full_name) { %>
                            <%= data.full_name %>
                                <% } else { %>
                                    Unlisted
                                    <% } %>
                    </div>
                    <% }) %>

                        <% } else { %>
                            <div class="empty-list">
                                <p>Momenteel in niemand afwezig</p>
                            </div>
                            <% } %>

        </div>

        <div class="content-box-4 content">
            <div class="content-title">
                <h2>Projecten</h2>
            </div>
            <% apiProjects.data.forEach(data=> { %>
                <div class="projects">
                    <div class="project-content">

                        <span class="project-date age">
                            <% if (data.start_date) { %>
                                <%= data.start_date.slice(8, 10) %>-<%= data.start_date.slice(6, 7) %>-<%=
                                            data.start_date.slice(0, 4) %>
                                            <% } else { %>
                                                undefiend
                                                <% } %>
                        </span>
                        <% if (data.name) { %>
                            <%= data.name %>
                                <% } else { %>
                                    undefiend
                                    <% } %>
                                        <span class="age">
                                            <p>
                                                <% if (data.organization.name) { %>
                                                    <%= data.organization.name %>
                                                        <% } else { %>
                                                            undefiend
                                                            <% } %>
                                            </p>

                                            <p> opdrachtnemer
                                                <% if (data.project_manager.name) { %>
                                                    <%= data.project_manager.name %>
                                                        <% } else { %>
                                                            undefiend
                                                            <% } %>
                                            </p>
                                        </span>

                    </div>
                </div>
                <% }) %>
        </div>

    </div>
    </div>
    <div class="background">
    </div>
</body>

</html>